	"version": "0.2.0",
	"configurations": [
		{
			"name": "(lldb) C",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceRoot}/grimly",
			"args": ["${workspaceFolder}/test.map"],
		},
		{
			"name": "(cppdbg) C",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/grimly",
			"args": ["${workspaceFolder}/test.map"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"filterStderr": true,
			"environment": [],
			"logging": {
				"traceResponse": true,
				"trace": true
			},
			"externalConsole": false,
			"MIMode": "lldb"
		}
	]
}